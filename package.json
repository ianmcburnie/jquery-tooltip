{
  "name": "jquery-tooltip",
  "description": "jQuery plugin that creates the basic interactivity for an ARIA tooltip widget",
  "version": "0.0.3",
  "scripts": {
    "prepublish": "npm run clean && npm run lint && npm run build",
    "build": "npm run lasso && npm run minify",
    "clean": "rimraf *.min.js lint.txt",
    "fix": "eslint jquery.tooltip.js --fix",
    "lasso": "lasso browser.json --output-dir ./ --minify",
    "lint": "eslint jquery.tooltip.js > lint.txt",
    "minify": "jsmin -o jquery.tooltip.min.js jquery.tooltip.js"
  },
  "pre-commit": "prepublish",
  "author": "Ian McBurnie <ianmcburnie@hotmail.com>",
  "homepage": "https://github.com/ianmcburnie/jquery-tooltip",
  "bugs": {
    "url": "https://github.com/ianmcburnie/jquery-tooltip/issues"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "accessibility",
    "a11y",
    "wai-aria",
    "tooltip"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcburnie/jquery-tooltip.git"
  },
  "license": "ISC",
  "devDependencies": {
    "eslint": "^2.12.0",
    "eslint-config-google": "^0.4.0",
    "jquery": ">=1.11",
    "jquery-focusable": "~0.3.0",
    "jquery-focus-exit": "~0.2.0",
    "jquery-migrate": "^1.4.1",
    "jquery-mouse-exit": "~0.2.0",
    "jquery-stick": "~0.1.0",
    "jsmin": "^1.0.1",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.5.2"
  },
  "peerDependencies": {
    "jquery": ">=1.11",
    "jquery-focusable": "~0.3.0",
    "jquery-focus-exit": "~0.2.0",
    "jquery-mouse-exit": "~0.2.0",
    "jquery-stick": "~0.1.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
